{
  "name": "@superdispatch/sdk",
  "version": "0.0.0",
  "private": false,
  "description": "Super Dispatch JavaScript SDK",
  "repository": "git@github.com:superdispatch/sdk.git",
  "license": "MIT",
  "author": "Umidbek Karimov <uma.karimov@gmail.com>",
  "main": "src/index.ts",
  "scripts": {
    "build": "npx pika build",
    "prebump": "npm run lint && npx tsc && npm run build",
    "bump": "npx pika publish --no-publish",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "lint": "js-tools lint",
    "tdd": "jest --watch",
    "test": "jest --coverage",
    "version": "npm run build"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-typescript": "^7.7.4",
    "@pika/cli": "^0.2.0",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.8.1",
    "@pika/plugin-build-types": "^0.8.1",
    "@pika/plugin-build-web": "^0.8.1",
    "@pika/plugin-bundle-types": "^0.8.1",
    "@pika/plugin-standard-pkg": "^0.8.1",
    "@superdispatch/eslint-plugin": "^0.1.30",
    "@superdispatch/js-tools": "^0.1.30",
    "@superdispatch/prettier-config": "^0.1.26",
    "@superdispatch/tsconfig": "^0.1.13",
    "@types/jest": "^24.0.23",
    "coveralls": "^3.0.9",
    "eslint": "^6.7.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.3"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*",
            "__testutils__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types",
        {
          "tsconfig": "./tsconfig.build.json"
        }
      ],
      [
        "@pika/plugin-bundle-types"
      ]
    ]
  }
}
